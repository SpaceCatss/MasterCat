    bb_set_color(button[i], c_white, .88);

    xx += bb_get_width(button[i]) + margin_x;
    if (xx&gt;total_width) {
        xx = xx_start;
        yy+=bb_get_height(button[i])+margin_y;
        bb_set_position(button[i], xx, yy);
        
        xx+=bb_get_width(button[i])+ margin_x;
    }
    
    
}


